# Remove Element prob-27

class Solution:
    def removeElement(self, nums: List[int], val: int) -> int:

        count = 0
        x = 0
        l= len(nums) - 1

        while x < len(nums):

            if nums[x] == val:
                count += 1
                nums[x] = nums[l]
                nums[l] = -55
                l -= 1
                x -= 1

            x+=1

        return len(nums) - count
