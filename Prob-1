 # Two sum prob-1

class Solution(object):

    def twoSum(self, nums, target):
         
        result=[]     # result list
    
        for i in range(len(nums)):
            
            for j in range(i+1,len(nums)):
                sum = nums[j] + nums[i]   
       
                if sum==target:        # Checking if the sum of i'th element and j'th element is the desired target
                    x.append(i)
                    x.append(j)
                        
        return x
