 # Two sum prob-1

class Solution(object):
    def twoSum(self, nums, target):

        x=[]
        for i in range(len(nums)):
            
            for j in range(i+1,len(nums)):
                sum=nums[j]+nums[i]
       
                if sum==target:
                    x.append(i)
                    x.append(j)
                        
        return x
