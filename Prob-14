#14. Longest Common Prefix

class Solution(object):
    def longestCommonPrefix(self, strs):
        
        st=""
        output=False
        min=9999999

        for i in strs:      # Trying to find the shortest string in the list because the longest common prefix a string in the list can have is the shortest string
                        
